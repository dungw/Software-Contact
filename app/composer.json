{
    "name": "Software Contact",
    "description": "Help people find the right software for their business",
    "keywords": ["software contact", "business software", "review software", "management software"],
    "homepage": "http://softwarecontact.com/",
    "type": "",
    "license": "",
    "support": {
        "issues": "http://softwarecontact.com/issues",
        "forum": "http://softwarecontact.com/forum",
        "wiki": "http://softwarecontact.com/wiki",
        "irc": "http://softwarecontact.com/irc",
        "source": "https://github.com/dungw/Software-Contact"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
